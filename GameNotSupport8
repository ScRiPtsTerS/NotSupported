LoadingFire.Name = "LoadingFire"
LoadingFire.Parent = MainLoad
LoadingFire.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
LoadingFire.BackgroundTransparency = 1.000
LoadingFire.Position = UDim2.new(0.712000072, 0, 0.673965991, 0)
LoadingFire.Rotation = 20.000
LoadingFire.Size = UDim2.new(0, 243, 0, 127)
LoadingFire.Visible = false
LoadingFire.Font = Enum.Font.LuckiestGuy
LoadingFire.Text = "ðŸ”¥"
LoadingFire.TextColor3 = Color3.fromRGB(255, 255, 255)
LoadingFire.TextSize = 100.000
wait(2.25)
local Loading = game:GetService("CoreGui").LoadingHub
local TextLabel = Loading.MainLoad.LoadingTitle
local Fire = Loading.MainLoad.LoadingFire
local Doneloading = 0

Loading.MainLoad:TweenSizeAndPosition(UDim2.new(0, 675,0, 411),UDim2.new(0.28, 0,0.198, 0),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.5,false) 
TextLabel:TweenSizeAndPosition(UDim2.new(0, 404,0, 70),UDim2.new(0.21, 0,0, 0),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.5,false)
Fire.Visible = true
repeat
    Loading.MainLoad.LoadingText.Text = "loading âœ¨GigaHubðŸ’°."
    wait(math.random(1,1.5))
    Loading.MainLoad.LoadingText.Text = "loading âœ¨GigaHubðŸ’°.."
    wait(math.random(1,1.5))
    Loading.MainLoad.LoadingText.Text = "loading âœ¨GigaHubðŸ’°..."
    wait(math.random(1,1.5))
    Loading.MainLoad.LoadingText.Text = "loading âœ¨GigaHubðŸ’°.."
    wait(math.random(1,1.5))
    Doneloading += 1
until
Doneloading == 3
if Doneloading == 3 then
    Loading.MainLoad.LoadingText:Destroy()
    Loading.MainLoad:TweenSizeAndPosition(UDim2.new(0, 387,0, 184),UDim2.new(0.392, 0,0.341, 0),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.5,false)
    TextLabel:TweenSizeAndPosition(UDim2.new(0, 243,0, 70),UDim2.new(0.192, 0,0, 0),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.5,false)
end
Fire.Visible = false
repeat wait() until Loading.MainLoad.Position == UDim2.new(0.392, 0,0.341, 0)
wait(2)
Loading:Destroy()
